{
    "name": "galaxy-it/exchange_1c",
    "type": "library",
    "description": "Catalog Loader from 1c - CommerceML with laravel",
    "keywords": [
        "1c",
        "CommerceML",
        "Laravel"
    ],
    "homepage": "https://github.com/Galaxy-IT",
    "license": "MIT",
    "authors": [
        {
            "name": "Developer",
            "role": "Developer"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Galaxy\\LaravelExchange1C\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {}
    },
    "require": {
        "php": ">=7.1",
        "carono/commerceml": "^0.2.3",
        "laravel/framework": "^5.4|^6.0|^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "orchestra/testbench": "~3.0",
        "mockery/mockery": "^1.2",
        "illuminate/session": "^5.7",
        "symfony/http-foundation": "^4.1",
        "symfony/event-dispatcher": "^4.1"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Galaxy\\LaravelExchange1C\\Exchange1CServiceProvider"
            ]
        }
    }
}